{
  "name": "FileObject",
  "schema": {
    "type": "object",
    "required": [
      "@context",
      "@id",
      "@type",
      "name"
    ],
    "properties": {
      "@context": {
        "type": "array",
        "items": {
          "oneOf": [{"type": "string"}, {"type": "object"}]
        },
        "default": [
          "https://schema.org/",
          {"isPartOf": {"@id": "https://schema.org/isPartOf", "@type": "@id"}}
        ],
        "cordra": {
          "disableInUi": true
        }
      },
      "@id": {
        "type": "string",
        "cordra": {
          "disableInUi": true,
          "type": {
            "autoGeneratedField": "handle"
          },
          "preview": {
            "showInPreview": true,
            "isPrimary": true
          }
        }
      },
      "@type": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": [
          "MediaObject"
        ]
      },
      "name": {
        "type": "string",
        "title": "File Name",
        "cordra": {
          "preview": {
            "showInPreview": true
          }
        }
      },
      "description": {
        "type": "string",
        "cordra": {
          "preview": {
            "showInPreview": false
          }
        }
      },
      "contentSize": {
        "type": "integer"
      },
      "encodingFormat": {
        "type": "string",
        "title": "Encoding",
        "cordra": {
          "preview": {
            "showInPreview": true
          }
        }
      },
      "isPartOf": {
        "type": "array",
        "format": "table",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "cordra": {
            "type": {
              "handleReference": {
                "types": [
                  "Dataset"
                ]
              }
            }
          }
        }
      },
      "contentUrl": {
        "type": "string"
      }
    }
  }
}