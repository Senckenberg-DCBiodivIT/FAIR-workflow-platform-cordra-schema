{
  "name": "FileObject",
  "schema": {
    "type": "object",
    "required": [
      "@id",
      "@type",
      "@context",
      "name"
    ],
    "properties": {
      "@id": {
        "type": "string",
        "cordra": {
          "disableInUi": true,
          "type": {
            "autoGeneratedField": "handle"
          },
          "preview": {
            "showInPreview": true,
            "isPrimary": true
          }
        }
      },
      "@type": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": [
          "MediaObject"
        ]
      },
      "@context": {
        "type": "object",
        "properties": {
          "@vocab": {
            "type": "string",
            "default": "https://schema.org/",
            "cordra": {
              "disableInUi": true
            }
          }
        }
      },
      "name": {
        "type": "string",
        "title": "File Name",
        "cordra": {
          "preview": {
            "showInPreview": true
          }
        }
      },
      "description": {
        "type": "string",
        "cordra": {
          "preview": {
            "showInPreview": false
          }
        }
      },
      "contentSize": {
        "type": "integer"
      },
      "encodingFormat": {
        "type": "string",
        "title": "Encoding",
        "cordra": {
          "preview": {
            "showInPreview": true
          }
        }
      },
      "partOf": {
        "type": "array",
        "format": "table",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "cordra": {
            "type": {
              "handleReference": {
                "types": [
                  "Dataset"
                ]
              }
            }
          }
        }
      },
      "resultOf": {
        "type": "string",
        "cordra": {
          "type": {
            "handleReference": {
              "types": [
                "CreateAction"
              ]
            }
          }
        }
      }
    }
  }
}