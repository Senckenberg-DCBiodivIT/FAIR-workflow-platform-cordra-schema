{
  "name": "Workflow",
  "schema": {
    "type": "object",
    "required": [
      "@context",
      "@id",
      "@type",
      "contentUrl",
      "programmingLanguage"
    ],
    "properties": {
      "@context": {
        "type": "array",
        "items": {
          "oneOf": [{"type": "string"}, {"type": "object"}]
        },
        "default": [
          "https://schema.org/",
          {"ComputationalWorkflow":  "https://bioschemas.org/ComputationalWorkflow"},
          {"partOf": {"@id": "https://schema.org/partOf", "@type": "@id"}},
          {"resultOf": {"@id": "https://schema.org/partOf", "@type": "@id"}},
          {"programmingLanguage": {"@id": "https://schema.org/programmingLanguage", "@type": "@id"}}
        ],
        "cordra": {
          "disableInUi": true
        }
      },
      "@id": {
        "type": "string",
        "cordra": {
          "disableInUi": true,
          "type": {
            "autoGeneratedField": "handle"
          }
        }
      },
      "@type": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": [
          "ComputationalWorkflow",
          "SoftwareSourceCode",
          "MediaObject"
        ]
      },
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "identifier": {
        "type": "string"
      },
      "programmingLanguage": {
        "type": "string"
      },
      "partOf": {
        "type": "string",
        "cordra": {
          "type": {
            "handleReference": {
              "types": [
                "Dataset"
              ]
            }
          }
        }
      },
      "contentUrl": {
        "type": "string"
      },
      "contentSize": {
        "type": "integer"
      },
      "encodingFormat": {
        "type": "string",
        "title": "Encoding",
        "cordra": {
          "preview": {
            "showInPreview": true
          }
        }
      }
    }
  }
}