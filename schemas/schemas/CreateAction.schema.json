{
  "name": "CreateAction",
  "schema": {
    "type": "object",
    "required": [
      "@context",
      "@id",
      "@type",
      "result"
    ],
    "properties": {
      "@context": {
        "type": "array",
        "items": {
          "oneOf": [{"type": "string"}, {"type": "object"}]
        },
        "default": [
          "https://schema.org/",
          {"agent": {"@id": "https://schema.org/agent", "@type": "@id"}},
          {"result": {"@id": "https://schema.org/result", "@type": "@id"}},
          {"object": {"@id": "https://schema.org/object", "@type": "@id"}},
          {"instrument": {"@id": "https://schema.org/instrument", "@type": "@id"}}
        ],
        "cordra": {
          "disableInUi": true
        }
      },
      "@id": {
        "type": "string",
        "cordra": {
          "disableInUi": true,
          "type": {
            "autoGeneratedField": "handle"
          }
        }
      },
      "@type": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": [
          "CreateAction"
        ]
      },
      "agent": {
        "type": "string",
        "cordra": {
          "type": {
            "handleReference": {
              "types": [
                "Person"
              ]
            }
          }
        }
      },
      "startTime": {
        "type": "string",
        "format": "date-time"
      },
      "endTime": {
        "type": "string",
        "format": "date-time"
      },
      "result": {
        "type": "array",
        "format": "table",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "cordra": {
            "type": {
              "handleReference": {
                "types": [
                  "FileObject",
                  "Dataset"
                ]
              }
            }
          }
        }
      },
      "object": {
        "type": "array",
        "format": "table",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "cordra": {
            "type": {
              "handleReference": {
                "types": [
                  "Dataset",
                  "FileObject",
                  "PropertyValue"
                ]
              }
            }
          }
        }
      },
      "instrument": {
        "type": "string",
        "cordra": {
          "type": {
            "handleReference": {
              "types": [
                "SoftwareApplication",
                "Workflow"
              ]
            }
          }
        }
      }
    }
  }
}